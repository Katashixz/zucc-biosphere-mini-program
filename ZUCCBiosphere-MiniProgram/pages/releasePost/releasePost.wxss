 
.releasePost{
    background-color:#EEEAE7;
    border-top-left-radius: 40rpx;
    border-top-right-radius: 40rpx;
    width: 100%;
    height: 100vh;
}
#themeSelc{
    line-height:80rpx;
    border-bottom:2rpx rgb(182, 165, 165) solid;
    /* border-top:2rpx #777 solid */
}
#themeSelc image{
    padding-left:15rpx;
    margin-top:20rpx;
    width:45rpx;
    height:45rpx
}
#themeSelc #context1{
    margin-right:15rpx
}
#themeSelc .top{
    position:relative;
    top:0;
    left:0;
    height:80rpx;
    border-radius:20rpx;
    padding:0 10rpx;
    z-index:10
}
#themeSelc .top .pic1{
    position:absolute;
    top:4rpx;
    left:4rpx
}
#themeSelc .top .select{
    position:absolute;
    top:4rpx;
    left:80rpx;
    font-size:30rpx;
    color:#474545
}
#themeSelc .top .selectColor{
    color:#474545
}
#themeSelc .top .top2{
    opacity:0;
    position:absolute;
    top:70rpx;
    width:100%;
    display:flex
}
#themeSelc .top .top2 .option{
    width:100%;
    color: #474545;
}
#themeSelc .top .top2 view{
    margin:20rpx;
    width:120rpx;
    float:left;
    text-align:center;
    background-color:#eee;
    border-radius:20rpx;
    overflow:hidden
}
#themeSelc .top .top2 .isSelect{
    /* border:2rpx #B6A5A5 solid; */
    background-color:rgb(215,193,168);
    color:#474545
}
#themeSelc .top .pic2{
    position:absolute;
    right:4rpx;
    top:4rpx
}
#themeSelc .top .pic2 #right1{
    margin-left:460rpx
}
#bodyText{
    padding-top:40rpx;
    background-color:#EEEAE7
}
#bodyText #textIn{
    height:250rpx;
    padding:0 40rpx
}
#bodyText #textIn textarea{
    width:100%;
    font-size:30rpx;
    color:#474545
}
#bodyText .pictureMngr{
    display:flex;
    flex-wrap:wrap
}
#bodyText .pictureMngr .imageList{
    margin:25rpx;
    width:180rpx;
    height:200rpx;
    border:3px solid #f0f0f0;
    border-radius:20rpx;
    overflow:hidden
}
#bodyText .pictureMngr .imageList .singerImage{
    z-index:10;
    height:200rpx;
    width:180rpx
}
#bodyText .pictureMngr .imageList .deleteImage{
    z-index:100;
    width:100rpx;
    height:100rpx;
    border-radius:100%;
    background:#2e82ff;
    position:relative;
    left:130rpx;
    bottom:250rpx
}
#bodyText .pictureMngr #addPho{
    margin:25rpx;
    height:200rpx;
    width:200rpx;
    text-align:center;
    display:flex;
    flex-direction:column;
    justify-content:center;
    /* background-color:#eee; */
    border-radius:20rpx;

}
#bodyText .pictureMngr #addPho image{
    margin-top:15rpx;
    margin-left:50rpx;
    height:100rpx;
    width:100rpx
}
#bodyText .pictureMngr #addPho view{
    margin-left:20rpx;
    width:150rpx;
    font-size:30rpx;
    font-weight: 800;
    border-radius:20rpx;
    border:3rpx rgb(215,193,168) solid;
    background-color:rgb(215,193,168);
    color:#fff
    
}
#textPrompt{
    margin-top:40rpx;
    padding:40rpx;
    font-size:28rpx;
    border-bottom:2rpx #B6A5A5 solid;
    border-top:2rpx #B6A5A5 solid;
    color: #474545;
    background-color:#EEEAE7;

}
#lineButtom{
    background-color:#EEEAE7;
    padding-top:60rpx;
    padding-bottom: 90rpx;
    /* margin-top:60rpx; */
    

}
#lineButtom .btn{
    background-color:#EEEAE7;
    width:300rpx;
    height:80rpx;
    border-radius:30rpx;
    font-size:36rpx;
    margin:auto;
    background-color:rgb(215,193,168);
    color:#fff
}/*# sourceMappingURL=./releasePost.wxss.map */