<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biosphere.library.mapper.PlantsWikiMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.biosphere.library.pojo.PlantsWiki">
        <id column="ID" property="id" />
        <result column="scientificNameID" property="scientificNameID" />
        <result column="nickName" property="nickName" />
        <result column="location" property="location" />
        <result column="image" property="image" />
        <result column="alias" property="alias" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, scientificNameID, nickName, location, imageUrl, alias
    </sql>

</mapper>
