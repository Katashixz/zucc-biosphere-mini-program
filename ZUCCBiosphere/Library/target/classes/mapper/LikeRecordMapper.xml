<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biosphere.library.mapper.LikeRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.biosphere.library.pojo.LikeRecord">
        <id column="id" property="id" />
        <result column="userID" property="userID" />
        <result column="postID" property="postID" />
        <result column="date" property="date" />
        <result column="ischecked" property="ischecked" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, userID, postID, date, ischecked
    </sql>

</mapper>
